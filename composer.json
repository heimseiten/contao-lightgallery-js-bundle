{
    "name": "heimseiten/contao-lightgallery-js-bundle",
    "description": "Adds lightgallery.js with video support. Commercial licencing: https://sachinchoolur.github.io/lightgallery.js/docs/license.html",
    "keywords": [
        "lightbox",
        "modal",
        "modalbox",
        "gallery",
        "galerie"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "heimseiten.de - Webdesign aus KÃ¶ln",
            "email": "info@heimseiten.de",
            "homepage": "https://www.heimseiten.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimseiten/contao-lightgallery-js-bundle/issues",
        "source": "https://github.com/heimseiten/contao-lightgallery-js-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.4"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^3.0",
        "contao/manager-plugin": "^2.3",
        "contao/test-case": "^4.0",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Heimseiten\\ContaoLightgalleryJsBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Heimseiten\\ContaoLightgalleryJsBundle\\ContaoManager\\Plugin"
    }
}